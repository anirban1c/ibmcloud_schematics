{
  "resource": {
    "ibm_is_public_gateway": {
      "iaas-dev-public-gw": {
        "depends_on": [
          "ibm_is_vpc.dev-vpc"
        ],
        "name": "iaas-dev-public-gw",
        "vpc": "${ibm_is_vpc.dev-vpc.id}",
        "zone": "eu-de-1"
      }
    },
    "ibm_is_subnet": {
      "subnet-1": {
        "depends_on": [
          "ibm_is_vpc.dev-vpc",
          "ibm_is_subnet.subnet-1"
        ],
        "ipv4_cidr_block": "${ibm_is_vpc_address_prefix.subnet-1-pfx.cidr}",
        "name": "subnet-1",
        "resource_group": "${data.ibm_resource_group.all_rg.id}",
        "timeouts": {
          "create": "10m",
          "delete": "10m"
        },
        "vpc": "${ibm_is_vpc.dev-vpc.id}",
        "zone": "eu-de-1"
      },
      "subnet-2": {
        "depends_on": [
          "ibm_is_vpc.dev-vpc",
          "ibm_is_subnet.subnet-2"
        ],
        "ipv4_cidr_block": "${ibm_is_vpc_address_prefix.subnet-2-pfx.cidr}",
        "name": "subnet-2",
        "resource_group": "${data.ibm_resource_group.all_rg.id}",
        "timeouts": {
          "create": "10m",
          "delete": "10m"
        },
        "vpc": "${ibm_is_vpc.dev-vpc.id}",
        "zone": "eu-de-1"
      }
    },
    "ibm_is_vpc_address_prefix": {
      "subnet-1-pfx": {
        "cidr": "192.168.1.0/24",
        "depends_on": [
          "ibm_is_vpc.dev-vpc"
        ],
        "name": "subnet-1",
        "vpc": "${ibm_is_vpc.dev-vpc.id}",
        "zone": "eu-de-1"
      },
      "subnet-2-pfx": {
        "cidr": "192.168.2.0/24",
        "depends_on": [
          "ibm_is_vpc.dev-vpc"
        ],
        "name": "subnet-2",
        "vpc": "${ibm_is_vpc.dev-vpc.id}",
        "zone": "eu-de-1"
      }
    }
  }
}